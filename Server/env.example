# Database Configuration - Use your external MongoDB URL
DATABASE_URL=mongodb+srv://username:password@cluster.mongodb.net/voting_app?retryWrites=true&w=majority

# Node Environment
NODE_ENV=production

# Server Configuration
PORT=5000

# JWT Configuration (if using JWT)
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# Email Configuration (if using nodemailer)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_email_password

# CORS Configuration
CORS_ORIGIN=http://localhost:3000 